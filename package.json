    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-graphql": "^0.4.9",
    "graphql": "^0.4.18",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.7.0",
    "material-ui": "^0.14.4",
    "mongoose": "^4.3.7",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth20": "^1.0.0",
