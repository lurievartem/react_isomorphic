{
  "name": "isomorphic-app",
  "version": "0.0.1",
  "description": "react redux express isomorphic app",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=development nodemon --debug server/index.js",
    "build": "cross-env NODE_ENV=production npm run clean && npm run build:webpack",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint shared client server",
    "lint:fix": "npm run lint -- --fix",
    "dev": "devtool server/index.js"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "node"
  ],
  "dependencies": {
