{
  "name": "isomorphic-app",
  "version": "0.0.1",
  "description": "react redux express isomorphic app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "nodemon --debug server/index.js --ignore ./shared/components",
    "build:webpack": "webpack -p",
    "build": "npm run clean && npm run build:webpack",
    "test": "karma start",
    "test:watch": "npm test -- --watch --no-single-run"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "node"
  ],
  "author": "Artem Luriev",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.2",
    "redux": "^3.0.6",
    "redux-thunk": "^1.0.3",
    "serve-static": "^1.10.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-runtime": "^5.8.20",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.4",
    "nodemon": "^1.8.1",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.5.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
