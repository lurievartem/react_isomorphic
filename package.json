{
  "name": "isomorphic-app",
  "version": "0.0.1",
  "description": "react redux express isomorphic app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "nodemon --debug server/index.js",
    "build:webpack": "webpack -p",
    "build": "set NODE_ENV=production npm run clean && npm run build:webpack",
    "test": "karma start",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "node"
  ],
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-graphql": "^0.4.5",
    "graphql": "^0.4.14",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.3.7",
    "path": "^0.12.7",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.2",
    "redux": "^3.0.6",
    "redux-thunk": "^1.0.3",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "file-loader": "^0.8.5",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.4",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.3",
    "rimraf": "^2.5.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lurievartem/react_isomorphic.git"
  },
  "author": "Artem Luriev",
  "license": "MIT"
}
